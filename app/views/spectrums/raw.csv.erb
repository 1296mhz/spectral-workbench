Wavelength,Average,Red,Green,Blue
<%

data = ActiveSupport::JSON.decode(@spectrum.clean_json)["lines"]

data.each do |datum|

w = datum['wavelength'].to_s[0..6]
a = datum['average'].to_s[0..3]
r = datum['r']
g = datum['g']
b = datum['b']
w = 0 if w == ""

%><%= w %>,<%= a %>,<%= r %>,<%= g %>,<%= b %>
<% end %>
